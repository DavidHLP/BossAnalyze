<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.david.hlp.Spring.crawler.boss.mapper.JobListMapper">
    <insert id="insert" parameterType="com.david.hlp.Spring.crawler.boss.model.JobList">
        INSERT INTO job_list (html_url, json_data, created_at, updated_at)
        VALUES (#{htmlUrl}, #{jsonData}, NOW(), NOW())
    </insert>
</mapper>

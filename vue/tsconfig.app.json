{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "jsx": "preserve",
    "resolveJsonModule": true,
    "isolatedModules": false,
    "esModuleInterop": true,
    "lib": [
      "ESNext",
      "DOM"
    ],
    "skipLibCheck": true,
    "noEmit": true,
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@@types/*": [
        "./types/*"
      ]
    },
    "types": [
      "element-plus/global"
    ]
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "src/router/modules/template.ts.not",
    "src/router/modules/home.ts.not",
    "src/router/modules/update.ts.not",
    "src/router/modules/update.ts.not"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}